package com.scan.annotate;

import java.lang.annotation.Annotation;
import java.lang.reflect.Method;
import java.util.ArrayList;
import java.util.List;


public class CapturedAnnotClzAndMethds {
	Class<?> anoClz ;
	List<Method> mthodLis  = new ArrayList<Method>();
	Class<? extends Annotation> annClz ;
	Class<? extends Annotation> annMthd ;


	public Class<?> getAnoClz() {
		return anoClz;
	}
	public void setAnoClz(Class<?> anoClz) {
		this.anoClz = anoClz;
	}
	public List<Method> getMthodLis() {
		return mthodLis;
	}
	public void setMthodLis(List<Method> mthodLis) {
		this.mthodLis = mthodLis;
	}
	public Class<? extends Annotation> getAnnClz() {
		return annClz;
	}
	public void setAnnClz(Class<? extends Annotation> annClz) {
		this.annClz = annClz;
	}
	public Class<? extends Annotation> getAnnMthd() {
		return annMthd;
	}
	public void setAnnMthd(Class<? extends Annotation> annMthd) {
		this.annMthd = annMthd;
	}
	
	
	
	
	

}

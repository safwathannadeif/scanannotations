package com.scan.annotate;

import java.lang.annotation.Annotation;
import java.util.ArrayList;
import java.util.List;

public class OutPutLisOfAnnotClzsAndMethds {
	List<CapturedAnnotClzAndMethds> mthodLis  = new ArrayList<CapturedAnnotClzAndMethds>();
	Class<? extends Annotation> annClz ;
	Class<? extends Annotation> annMthd ;
	public List<CapturedAnnotClzAndMethds> getMthodLis() {
		return mthodLis;
	}
	public void setMthodLis(List<CapturedAnnotClzAndMethds> mthodLis) {
		this.mthodLis = mthodLis;
	}
	public Class<? extends Annotation> getAnnClz() {
		return annClz;
	}
	public void setAnnClz(Class<? extends Annotation> annClz) {
		this.annClz = annClz;
	}
	public Class<? extends Annotation> getAnnMthd() {
		return annMthd;
	}
	public void setAnnMthd(Class<? extends Annotation> annMthd) {
		this.annMthd = annMthd;
	}
	@Override
	public String toString() {
		return "OutPutLisOfAnnotClzsAndMethds [mthodLis=" + mthodLis + ", annClz=" + annClz + ", annMthd=" + annMthd
				+ "]";
	}
}
